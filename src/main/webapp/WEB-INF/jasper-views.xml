<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="kendaraanPdf" class="org.springframework.web.servlet.view.jasperreports.JasperReportsPdfView" p:url="classpath:/report/master_kendaraan.jrxml"
		p:reportDataKey="dataSource" />

	<bean id="newsPdf" class="org.springframework.web.servlet.view.jasperreports.JasperReportsPdfView" p:url="classpath:/report/news.jrxml"
		p:reportDataKey="dataSource" />

	<bean id="newsDetailPdf" class="org.springframework.web.servlet.view.jasperreports.JasperReportsPdfView" p:url="classpath:/report/news_detail.jrxml"
		p:reportDataKey="dataSource" />
</beans>
